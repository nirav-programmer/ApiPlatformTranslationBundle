{
  "name": "locastic/api-platform-translation-bundle",
  "description": "Translation bundle for Api platform based on Sylius translation",
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Paula",
      "email": "paula@locastic.com"
    }
  ],
  "require": {
    "php": ">=8.4",
    "api-platform/symfony": "^4.2",
    "doctrine/orm": "^3.5",
    "doctrine/doctrine-bundle": "^3.1",
    "symfony/translation": "*",
    "symfony/dependency-injection": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Locastic\\ApiPlatformTranslationBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Locastic\\ApiPlatformTranslationBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "conflict": {
    "symfony/translations-contracts": "<2.0"
  }
}
